[Unit]
Description=Chemical Thinking API Server
After=network.target ollama.service

[Service]
Type=simple
User=kiran
WorkingDirectory=/storage/chem-math-course/app/backend
ExecStart=/usr/bin/python3 main.py
Restart=always
RestartSec=3
Environment=PYTHONUNBUFFERED=1

[Install]
WantedBy=multi-user.target
